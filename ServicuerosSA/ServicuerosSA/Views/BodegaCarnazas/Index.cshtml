@model IEnumerable<ServicuerosSA.Models.BodegaCarnaza>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Cantidad)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Codigo)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.BodegaGeneral)
