            <th>
                @Html.DisplayNameFor(model => model.Bodegatripas)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Cantidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Codigo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BodegaGeneral.NombreBodega)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Bodegatripas.BodegaTripaId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.BodegaCarnazaId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.BodegaCarnazaId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.BodegaCarnazaId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
